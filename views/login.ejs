                <br>
                <div class="flex flex-middle">
                    <a class="" id="loginBtn" href="/auth/google">Login with google</a>
                </div>
                
                
                
            </div>
        </div>
        <%- include ('./base/bottom') %>
    </div>
    
</body>
</html>